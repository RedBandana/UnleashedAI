{
  "name": "gpt-unleashed",
  "version": "0.1.0",
  "author": "Diamond Soul",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "@capacitor/android": "^5.0.5",
    "@capacitor/clipboard": "^5.0.5",
    "@capacitor/core": "^5.0.5",
    "@capacitor/ios": "^5.0.5",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-redux": "^8.1.1",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "remark-gfm": "^3.0.1",
    "sass": "^1.62.0",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "appId": "com.diamond-soul.gpt-unleashed",
    "productName": "GPT Unleashed",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "main.js"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "icon": "assets/icon.png"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && uglifyjs build/static/js/*.js -o build/static/js/app.min.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "electron .",
    "electron:build": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.0.5",
    "electron": "^24.3.1",
    "electron-builder": "^23.6.0",
    "uglify-js": "^3.17.4"
  }
}
